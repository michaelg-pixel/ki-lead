# .htaccess fÃ¼r Freebie-Bereich
RewriteEngine On
RewriteBase /freebie/

# Regel 1: Wenn die Anfrage auf .php endet -> durchlassen
RewriteCond %{REQUEST_URI} \.php$
RewriteRule ^ - [L]

# Regel 2: Alles andere (32-stellige hex IDs) -> zu index.php weiterleiten
RewriteRule ^([a-f0-9]{32})$ index.php?id=$1 [L,QSA]